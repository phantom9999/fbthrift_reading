    }}{{#value:integer?}}{{value:integer_value}}.0{{/value:integer?}}{{!
  }}{{/type:double?}}{{!
  }}{{#type:enum?}}{{!
    }}{{#type:enum}}{{!
      }}{{enum:go_qualified_name}}{{!
    }}{{/type:enum}}{{!
